{
  "name": "backlog",
  "private": true,
  "version": "0.2.0",
  "description": "A simple markup language for Backlog",
  "type": "module",
  "packageManager": "pnpm@7.25.0",
  "keywords": [
    "backlog",
    "markdown",
    "table"
  ],
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/backlog-parser.umd.cjs",
  "module": "./dist/backlog-parser.js",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/backlog-parser.js",
    "require": "./dist/backlog-parser.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@vueuse/core": "^9.12.0",
    "codemirror": "^6.0.1",
    "element-plus": "^2.2.28",
    "jsonlint": "^1.6.3",
    "vue": "^3.2.45",
    "vue-codemirror": "^6.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "jison": "^0.4.18",
    "postcss": "^8.4.21",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-icons": "^0.15.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.0",
    "vitest": "^0.28.3",
    "vue-tsc": "^1.0.11"
  }
}